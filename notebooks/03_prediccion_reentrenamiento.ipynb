   "source": [
    "## 9. Funci√≥n de Reentrenamiento"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "431c953a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def retrain_model(model, new_images, new_labels, epochs=30, batch_size=4, learning_rate=0.0001):\n",
    "    \"\"\"\n",
    "    Reentrena el modelo con nuevas im√°genes.\n",
    "    \n",
    "    Args:\n",
    "        model: Modelo de Keras a reentrenar\n",
    "        new_images: Array numpy de im√°genes (N, 224, 224, 3)\n",
    "        new_labels: Array numpy de etiquetas (N, num_classes)\n",
    "        epochs: N√∫mero de epochs para reentrenamiento\n",
    "        batch_size: Tama√±o del batch\n",
    "        learning_rate: Tasa de aprendizaje\n",
    "    \n",
    "    Returns:\n",
    "        Modelo reentrenado y historial de entrenamiento\n",
    "    \"\"\"\n",
    "    # Verificar datos\n",
    "    assert new_images.dtype == np.float32, \"Im√°genes deben ser float32\"\n",
    "    assert new_labels.dtype == np.float32, \"Etiquetas deben ser float32\"\n",
    "    \n",
    "    # Replicar datos para tener m√°s muestras (data augmentation simple)\n",
    "    X_rep = np.tile(new_images, (10, 1, 1, 1))\n",
    "    y_rep = np.tile(new_labels, (10, 1))\n",
    "    \n",
    "    print(f\"Reentrenando con {len(new_images)} im√°genes originales ({len(X_rep)} con replicaci√≥n)...\")\n",
    "    \n",
    "    # Recompilar modelo con learning rate bajo\n",
    "    model.compile(\n",
    "        optimizer=keras.optimizers.Adam(learning_rate=learning_rate),\n",
    "        loss='binary_crossentropy',\n",
    "        metrics=['accuracy']\n",
    "    )\n",
    "    \n",
    "    # Entrenar\n",
    "    history = model.fit(\n",
    "        X_rep, y_rep,\n",
    "        batch_size=batch_size,\n",
    "        epochs=epochs,\n",
    "        verbose=1\n",
    "    )\n",
    "    \n",
    "    return model, history\n",
    "\n",
    "def save_retrained_model(model, save_mlflow=True):\n",
    "    \"\"\"\n",
    "    Guarda el modelo reentrenado.\n",
    "    \n",
    "    Args:\n",
    "        model: Modelo de Keras\n",
    "        save_mlflow: Si True, tambi√©n guarda en MLflow\n",
    "    \"\"\"\n",
    "    # Guardar localmente\n",
    "    model_path = os.path.join(MODELS_DIR, 'best_model.keras')\n",
    "    model.save(model_path)\n",
    "    print(f\"‚úÖ Modelo guardado localmente: {model_path}\")\n",
    "    \n",
    "    # Guardar en MLflow\n",
    "    if save_mlflow:\n",
    "        try:\n",
    "            with mlflow.start_run(run_name=\"Retrained\"):\n",
    "                mlflow.log_param(\"model\", \"Retrained\")\n",
    "                mlflow.log_param(\"type\", \"retraining\")\n",
    "                mlflow.keras.log_model(\n",
    "                    model,\n",
    "                    artifact_path=\"model\",\n",
    "                    registered_model_name=\"Retrained\"\n",
    "                )\n",
    "                mlflow.set_tag(\"retrained\", \"True\")\n",
    "            print(f\"‚úÖ Modelo guardado en MLflow\")\n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Error guardando en MLflow: {e}\")\n",
    "\n",
    "print(\"‚úÖ Funciones de reentrenamiento definidas\")"
   ]
