    "def load_best_model():\n",
    "    \"\"\"\n",
    "    Carga el √∫ltimo modelo (m√°s reciente) desde mlflow_data.\n",
    "    \n",
    "    Returns:\n",
    "        Modelo de Keras cargado\n",
    "    \"\"\"\n",
    "    try:\n",
    "        print(f\"Buscando modelos en: {MLFLOW_DATA_DIR}\")\n",
    "        \n",
    "        # Configurar MLflow para usar mlflow_data\n",
    "        mlflow.set_tracking_uri(f\"file://{str(MLFLOW_DATA_DIR.absolute())}\")\n",
    "        \n",
    "        # Buscar experimento\n",
    "        experiments = mlflow.search_experiments()\n",
    "        if not experiments:\n",
    "            raise Exception(\"No se encontraron experimentos en mlflow_data\")\n",
    "        \n",
    "        # Usar el primer experimento\n",
    "        exp_id = experiments[0].experiment_id\n",
    "        print(f\"Usando experimento: {experiments[0].name} (ID: {exp_id})\")\n",
    "        \n",
    "        # Buscar runs ordenados por fecha (m√°s reciente primero)\n",
    "        runs = mlflow.search_runs(\n",
    "            experiment_ids=[exp_id],\n",
    "            order_by=[\"start_time DESC\"],\n",
    "            max_results=1\n",
    "        )\n",
    "        \n",
    "        if len(runs) == 0:\n",
    "            raise Exception(\"No se encontraron runs en mlflow_data\")\n",
    "        \n",
    "        # Cargar el modelo m√°s reciente\n",
    "        best_run = runs.iloc[0]\n",
    "        run_id = best_run['run_id']\n",
    "        run_name = best_run.get('tags.mlflow.runName', 'Unknown')\n",
    "        start_time = best_run.get('start_time', 'Unknown')\n",
    "        \n",
    "        print(f\"Cargando modelo m√°s reciente:\")\n",
    "        print(f\"  - Run: {run_name}\")\n",
    "        print(f\"  - ID: {run_id}\")\n",
    "        print(f\"  - Fecha: {start_time}\")\n",
    "        \n",
    "        # Intentar cargar el modelo\n",
    "        try:\n",
    "            model = mlflow.keras.load_model(f\"runs:/{run_id}/model\")\n",
    "            print(f\"‚úÖ Modelo cargado desde mlflow_data\")\n",
    "            return model\n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Error cargando modelo con artifact 'model', intentando alternativas...\")\n",
    "            # Intentar con otros nombres de artifact\n",
    "            client = mlflow.tracking.MlflowClient()\n",
    "            artifacts = client.list_artifacts(run_id)\n",
    "            for artifact in artifacts:\n",
    "                if 'model' in artifact.path.lower():\n",
    "                    try:\n",
    "                        model = mlflow.keras.load_model(f\"runs:/{run_id}/{artifact.path}\")\n",
    "                        print(f\"‚úÖ Modelo cargado desde {artifact.path}\")\n",
    "                        return model\n",
    "                    except:\n",
    "                        continue\n",
    "            raise e\n",
    "            \n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Error cargando modelo: {e}\")\n",
    "        raise FileNotFoundError(\n",
    "            \"No se pudo cargar ning√∫n modelo desde mlflow_data.\\n\"\n",
    "            \"Aseg√∫rate de que existan modelos entrenados en mlflow_data/.\"\n",
    "        )"
   ]
